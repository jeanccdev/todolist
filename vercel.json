{
    "version": 2,
    "builds": [
      { "src": "data/db.json", "use": "@vercel/static" }
    ],
    "routes": [
      {
        "src": "/server",
        "dest": "data/db.json"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "env": {
      "SERVER_PORT": "5000"
    },
    "functions": {
        "api/**/*.js": {
          "runtime": "vercel-node@14",
          "memory": 128,
          "maxDuration": 10
        }
    }
}
  